{
  "name": "adrosoftware/fluent",
  "description": "A fluent PHP mapping driver for Doctrine2.",
  "license": "MIT",
  "keywords": [
    "doctrine",
    "orm",
    "data mapper",
    "database",
    "mappings",
    "mapping driver",
    "fluent"
  ],
  "authors": [
    {
      "name": "Guido Contreras Woda",
      "email": "guiwoda@gmail.com"
    },
    {
      "name": "Patrick Brouwers",
      "email": "patrick@maatwebsite.nl"
    },
    {
      "name": "Adro Rocker",
      "email": "me@adro.rocks",
      "homepage": "https://github.com/adrorocker"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "doctrine/orm": "2.5.*|2.6.*",
    "doctrine/inflector": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "~0.9",
    "beberlei/doctrineextensions": "~1.0",
    "zf1/zend-date": "~1.12",
    "nesbot/carbon": "*",
    "gedmo/doctrine-extensions": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "AdroSoftware\\Fluent\\": "src/",
      "Gedmo\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
